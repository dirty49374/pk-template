{
    "name": "pk-template",
    "version": "0.1.34",
    "description": "p template engine for kubernetes",
    "main": "dist/pk-lib/index.js",
    "scripts": {
        "start": "ts-node bin/pkt.ts",
        "watch": "npm run distclean && tsc --watch",
        "build": "npm run distclean && webpack --config webpack.config.js",
        "deploy": "npm run distclean && webpack --config webpack.config.js && npm publish",
        "webpack": "webpack --config webpack.config.js",
        "distclean": "rimraf dist/*",
        "test": "mocha"
    },
    "bin": {
        "pkt": "bin/pkt.js",
        "pkctl": "bin/pkctl.js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/dirty49374/pk-template.git"
    },
    "keywords": [
        "pk-template",
        "pkt",
        "k8s",
        "yaml",
        "template",
        "kubernetes"
    ],
    "author": "dirty49374@gmail.com",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/dirty49374/pk-template/issues"
    },
    "homepage": "https://github.com/dirty49374/pk-template#readme",
    "dependencies": {
        "@types/readline-sync": "^1.4.3",
        "ajv": "^6.6.1",
        "chalk": "^2.4.1",
        "coffeescript": "^2.3.2",
        "colors": "^1.3.3",
        "diff": "^4.0.1",
        "js-yaml": "^3.12.1",
        "json-patch": "^0.7.0",
        "jsonpath": "^1.0.0",
        "livescript": "^1.6.0",
        "readline-sync": "^1.4.9",
        "sync-request": "^6.0.0",
        "underscore": "^1.9.1",
        "yargs": "^12.0.5"
    },
    "devDependencies": {
        "@types/ajv": "^1.0.0",
        "@types/diff": "^4.0.1",
        "@types/js-yaml": "^3.12.0",
        "@types/json-patch": "0.0.30",
        "@types/jsonpath": "^0.2.0",
        "@types/node": "^11.9.4",
        "@types/underscore": "^1.8.9",
        "mocha": "^5.2.0",
        "ts-loader": "^5.3.3",
        "typescript": "^3.3.3",
        "webpack": "^4.29.3",
        "webpack-cli": "^3.2.3",
        "webpack-node-externals": "^1.7.2"
    }
